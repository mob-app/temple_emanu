<?xml version='1.0' encoding='utf-8'?>
<widget id="com.proj.temple emanu-el" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Temple Emanu-El</name>
    <description>
        Temple Emanu-El mobile app
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Ecrubit
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" /> 
    <!--<plugin name="org.li8.inappbrowser" spec="0.1" source="pgb" />-->
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
 
    <icon src="images/icons/launcher.png" platform="android" width="57" height="57" />
    <icon src="images/icon/logo_72.png" platform="android" width="72" height="72" />
    <icon src="images/icons/logo_76.png" platform="android" width="76" height="76" />
    <icon src="images/icons/logo_2x.png" platform="android" width="114" height="114" />
    <icon src="images/icons/logo_72_at_2x.png" platform="android" width="144" height="144" />
    <icon src="images/icons/logo_120.png" platform="android"  width="120" height="120" />
    <icon src="images/icons/loge_76_at_2x.png" platform="android" width="152" height="152" />
    
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>


	<access origin="*" subdomains="true"/>
	<allow-navigation href="*" />
	<preference name="Orientation" value="portrait" /> 
</widget>
